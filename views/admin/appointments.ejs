<!DOCTYPE html>
<html data-wf-page="5ffbd87001b4d60de6b37258" data-wf-site="5fde0bc9a49dc57bc05fab17">
<head>
  <meta charset="utf-8">
  <title>Appointments</title>
  <meta content="Appointments" property="og:title">
  <meta content="Appointments" property="twitter:title">
  <meta content="width=device-width, initial-scale=1" name="viewport">
  <meta content="Webflow" name="generator">
  <link href="/css/normalize.css" rel="stylesheet" type="text/css">
  <link href="/css/webflow.css" rel="stylesheet" type="text/css">
  <link rel="stylesheet" href="/css/alertify.min.css" />
  <link rel="stylesheet" href="/css/themes/default.min.css" />
  <link href="/css/cruspo-live.webflow.css" rel="stylesheet" type="text/css">
  <script type="text/javascript">!function(o,c){var n=c.documentElement,t=" w-mod-";n.className+=t+"js",("ontouchstart"in o||o.DocumentTouch&&c instanceof DocumentTouch)&&(n.className+=t+"touch")}(window,document);</script>
  <link href="/images/favicon.ico" rel="shortcut icon" type="image/x-icon">
  <link href="/images/webclip.png" rel="apple-touch-icon">
</head>
<body>
  <div class="course-creator-section">
    <div style="display:none" class="create-appointment-form-wrapper">
      <div class="create-appointment-form-box">
        <div class="create-course-image-wrapper thumbnail appointment">
          <div class="upload-appointment-thumbnail-button-wrapper"><img src="https://d3e54v103j8qbb.cloudfront.net/plugins/Basic/assets/placeholder.60f9b1840c.svg" loading="lazy" width="122" alt="" class="thumbnail-image"><img src="/images/upload-thumbnail.svg" loading="lazy" width="87" alt="" class="image-32">
            <div class="text-block-49">Upload Appointment Thumbnail</div>
          </div>
          <div class="cretae-course-form-image"><img src="/images/cruspo-extra.svg" loading="lazy" width="340" alt=""></div>
        </div>
        <div class="w-form">
          <form id="email-form" name="email-form" data-name="Email Form" enctype="multipart/form-data">
            <input type="text" class="w-input" maxlength="256" name="appointmentTitle" data-name="Appointment Title" placeholder="Give a title to appointment" id="Appointment-Title" required="">
            <textarea placeholder="Give a description about this appointment " maxlength="5000" id="Appointment-Description" name="appointmentDescription" data-name="Appointment Description" required="" class="w-input"></textarea>
            <input type="file" name="appointmentThumbnail" class="fileUpload" style="display: none;">
            <div class="appointment-price-wrapper">
              <select id="price-currency" name="currency" data-name="price-currency" required="" class="select-field-2 w-select">
                <option >select currency</option>
                <% currencies.forEach((currency)=>{%>
                  <option value="<%= JSON.stringify(currency) %>"><%= currency['fullName']+' ('+currency['symbol'] +')'%></option>
                <% }); %>
                </select>
                <input type="text" class="text-field-9 appointment-price w-input" maxlength="256" name="appointmentPrice" data-name="Appointment Price" placeholder="Price for appointment" id="Appointment-Price" required=""></div>
          </form>
        </div>
        <div class="next-appointment-button-wrapper">
          <!-- <div class="error-box">
            <div class="error-image-wrapper"><img src="/images/error.svg" loading="lazy" width="27" alt=""></div>
            <div class="error-text-wrapper">
              <h2 class="heading-26">An Error Occurred</h2>
              <div class="text-block-124">This is some text inside of a div block.</div>
            </div>
          </div> -->
          <div class="next-appointment">
            <a class="next-appointment-button w-inline-block">
              <div class="text-block-50">Next</div><img src="/images/dropdown-arrow.svg" loading="lazy" width="12" alt="dropdown-arrow
" class="image-13">
            </a>
          </div>
        </div>
        <div class="cancel-appointment-creation-button-wrapper-cop">
          <div data-w-id="c533afbb-71a7-0621-2f39-bc13fb3d753e" class="cancel-appointment-creation-button">
            <div>Cancel</div><img src="/images/add-member.svg" loading="lazy" alt="" class="cancel-icon">
          </div>
        </div>
      </div>
    </div>
    <div class="appointments-content-wrapper">
      <div data-duration-in="300" data-duration-out="100" class="tabs-6 w-tabs">
        <div class="appointment-tab-menu w-tab-menu">
          <a data-w-tab="Tab 1" class="create-appointment-link w-inline-block w-tab-link w--current">
            <div>Appointments</div>
          </a>
          <a data-w-tab="Tab 2" class="upcoming-appointment-link w-inline-block w-tab-link">
            <div>Upcoming Appointments</div>
          </a>
          <a data-w-tab="Tab 3" class="past-appointment-link w-inline-block w-tab-link">
            <div>Past Appointments</div>
          </a>
        </div>
        <div class="appointment-tab-content w-tab-content">
          <div data-w-tab="Tab 1" class="create-appointment-tab w-tab-pane w--tab-active">
            <div class="create-appointment-button-wrapper">
              <div data-w-id="cd028774-c25b-abf7-6dc2-be30bb61f542" class="create-appointment-button">
                <div class="text-block-48">Create Appointment</div><img src="/images/add-member.svg" loading="lazy" alt="">
              </div>
            </div>
            <div class="appointment-wrapper">
              <div class="w-layout-grid appointment-grid">
                <% appointments.forEach(a=>{ %>
                <div class="appointment-details-overview">
                  <div class="appointment-thumbnail">
                    <img src="<%=a['appointmentThumbnail']%>" loading="lazy" width="122" alt="" class="thumbnail-image"></div>
                  <div class="appointment-details-wrapper">
                    <h3 class="appointment-title"><%=a['appointmentTitle']%></h3>
                    <div class="appointmnt-status-wrapper">
                      <div class="appointment-published-status-wrapper">
                        <div class="appointment-status-color"></div>
                        <div class="text-block-52">Published</div>
                      </div>
                      <div class="appointment-not-published-status-wrapper">
                        <div class="appointment-status-color not-published"></div>
                        <div class="text-block-52">Not Published</div>
                      </div>
                    </div>
                    <div class="appointment-data">
                      <div class="appointment-duration-wrapper">
                        <div class="text-block-21">Duration:</div>
                        <div class="course-publish-dates">30 mins</div>
                      </div>
                      <div class="apoointment-edit-date-wrapper">
                        <div>Last Edited </div>
                        <div class="course-edit-dates"></div>
                      </div>
                    </div>
                    <div class="creator-name-price-appointment-wrapper">
                      <div class="course-creator-name-pic">
                        <div class="course-creator-profile-pic">
                          <img src="/images/sample-profile-pic.jpeg" loading="lazy" width="122" alt="" class="thumbnail-image"></div>
                        <div class="course-creator-name">Aditya Joshi</div>
                      </div>
                      <div class="appointment-price"><%= a['currency']['symbol']+' '+a['appointmentPrice']%></div>
                    </div>
                    <div class="appointment-details-button-wrapper">
                      <a href="appointment/edit/<%=a['_id']%>" class="cruspo-temp-button edit-appointment w-inline-block">
                        <div class="text-block-4 appointment">Edit Appointment</div><img src="/images/arrow-cruspo.svg" loading="lazy" alt="">
                      </a>
                      <a data-w-id="5c3e2793-2ee9-08df-ce1a-eb753f3fc0c9" href="#" class="share-appointment-session-button-cop w-inline-block">
                        <div class="text-block-4 share appointment">Share Info</div><img src="/images/share-button.svg" loading="lazy" width="18" alt="" class="image-4">
                      </a>
                    </div>
                  </div>
                </div>
                <% }); %>
              </div>
            </div>
            <% if(appointments.length==0){%>
            <div class="no-appointment-wrapper">
              <div class="text-block-31">You haven&#x27;t created any appointments so far. Create your first appointment by clicking the button below</div>
              <div class="create-appointment-button-wrapper">
                <div data-w-id="2cf2a99c-d0b3-6f50-0012-33bd3dbf02c8" class="create-appointment-button">
                  <div class="text-block-48">Create Appointment</div><img src="/images/add-member.svg" loading="lazy" alt="">
                </div>
              </div>
            </div>
            <% } %>
          </div>
          <div data-w-tab="Tab 2" class="upcoming-appointment-session-tab w-tab-pane">
            <div class="upcoming-appointment-session-wrapper">
              <!-- <div class="upcoming-appointment-session-header">
                <div class="upcoming-live-session-day">Monday</div>
                <div class="upcoming-live-session-date">4 January 2020</div>
                <div class="calendar-filter-wrapper"><img src="/images/calendar-icon.svg" loading="lazy" alt=""></div>
              </div> -->
              <div class="w-layout-grid session-grid">
                <div class="session-wrapper">
                  <div data-w-id="130b5e15-b776-f018-3770-fc7af901e9dc" class="session-header-wrapper appointment"><img src="/images/live-event.svg" loading="lazy" width="21" alt="">
                    <div class="session-title">This is some text inside of a div block.</div>
                    <div class="session-time">This is some text inside of a div block.</div>
                    <div class="session-duration">This is some text inside of a div block.</div><img src="/images/add-member.svg" loading="lazy" alt="" class="session-image">
                  </div>
                  <div style="height:0px;display:none" class="session-details-wrapper">
                    <div class="session-details-button">
                      <a href="#" class="cruspo-temp-button edit-live-session w-inline-block">
                        <div class="text-block-4">Edit Appointment</div><img src="/images/pen.svg" loading="lazy" width="14" alt="">
                      </a>
                      <a href="#" class="cruspo-temp-button edit-live-session w-inline-block">
                        <div class="text-block-4">Edit Appointment Type</div><img src="/images/pen.svg" loading="lazy" width="14" alt="">
                      </a>
                    </div>
                    <div class="session-details">
                      <div class="text-block-126">session title:</div>
                      <div class="text-block-127">session title</div>
                      <div class="text-block-126">session description:</div>
                      <div class="text-block-127">session description</div>
                      <div class="text-block-126">day/date:</div>
                      <div class="text-block-127">day/date of session</div>
                      <div class="text-block-126">time:</div>
                      <div class="text-block-127">session time (time zone)</div>
                      <div class="text-block-126">duration:</div>
                      <div class="text-block-127">session duration</div>
                      <div class="text-block-126">price:</div>
                      <div class="text-block-127">session price</div>
                      <div class="text-block-128">attendee details</div>
                      <div class="text-block-126">first name:</div>
                      <div class="text-block-127">firts name of attendee</div>
                      <div class="text-block-126">last name:</div>
                      <div class="text-block-127">last name of attendee</div>
                      <div class="text-block-126">email:</div>
                      <div class="text-block-127">email of attendee</div>
                      <div class="text-block-126">time zone:</div>
                      <div class="text-block-127">time zone of attendee</div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="no-upcoming-appointment-wrapper">
                <div class="text-block-31">There&#x27;s no upcoming appointment session.<br>All your upcoming appointments will appear here.</div>
              </div>
            </div>
          </div>
          <div data-w-tab="Tab 3" class="past-appointment-session-tab w-tab-pane">
            <div class="past-appointment-session-wrapper-cop">
              <!-- <div class="past-appointment-session-header">
                <div class="past-appointment-session-day-cop">Monday</div>
                <div class="past-appointment-session-date-copy">4 January 2020</div>
                <div class="calendar-filter-wrapper"><img src="/images/calendar-icon.svg" loading="lazy" alt=""></div>
              </div> -->
              <div class="no-past-appointment-wrapper">
                <div class="text-block-31">There&#x27;s no past appointment session.<br>All your past appointments will appear here.</div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <%- include('sidebar');%>
  </div>
  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=5fde0bc9a49dc57bc05fab17" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
  <script src="/js/webflow.js" type="text/javascript"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.21.1/axios.min.js" integrity="sha512-bZS47S7sPOxkjU/4Bt0zrhEtWx0y0CRkhEp8IckzK+ltifIIE9EMIMTuT/mEzoIMewUINruDBIR/jJnbguonqQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
  <script src="/js/appointment.js" type="text/javascript"></script>
  <script src="/js/alertify.min.js"></script>
  <script>
    $('.share-appointment-session-button-cop').click(async function(e){
      await navigator.clipboard.writeText(e.currentTarget.id);
      alertify.notify("Copied to clipboard","success");
    });
  </script>
</body>
</html>