                    <option value="">Select one.</option>
                    <option value="First">First Choice</option>
                    <option value="Second">Second Choice</option>
                    <option value="Third">Third Choice</option>
                  </select><img src="/images/calendar-icon.svg" loading="lazy" width="24" alt="">
                  <div class="text-block-100">*This period will begin after enrollment</div>
                </div>
              </div>
            </div>
          </form>
        </div>
      </div>
