                    <option value="day">Day</option>
                    <option value="month">Month</option>
                    <option value="week">Week</option>
                    <option value="year">Year</option>
                  </select>
                  <div class="text-block-74">*In what duration you want to collect program price</div>
                </div>
              </div>
            </div>
            <div class="course-expiry-date-wrapper">
              <div class="expiry-date-text-wrapper">
                <div class="text-block-104">Program is valid till <span class="text-span">*optional</span></div>
                <div>After the end of this date no one will be able to enroll into your program</div>
              </div>
              <div class="expiry-date-input-wrapper">
                <input type="date" class="expiry-date-input-field w-input" maxlength="256" name="courseExpiryDate" data-name="courseExpiryDate" value="<%if(course['courseExpiryDate']){
                  let date = new Date(course['courseExpiryDate']);
                    year = date.getFullYear();
                    month = date.getMonth()+1;
                    dt = date.getDate();
  
                    if (dt < 10) {
                      dt = '0' + dt;
                    }
                    if (month < 10) {
                      month = '0' + month;
                    }
                    let d = year+'-' + month + '-'+dt;%><%=d%><%}%>" placeholder="Course is valid till." id="courseExpiryDate"><img src="/images/calendar-icon.svg" loading="lazy" width="26" alt=""></div>
            </div>
