      <div class="w-form">
        <input type="fileUpload" name="appointmentThumbnail" style="visibility: hidden;">
        <form data-name="Email Form">
          <input type="text" class="w-input" maxlength="256" name="appointmentTitle" value="<%= appointment['appointmentTitle']%>" data-name="Appointment Title" placeholder="Give a title to appointment" id="Appointment-Title" required="">
