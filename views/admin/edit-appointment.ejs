          <div class="appointment-price-wrapper">
            <select id="price-currency" name="currency" data-name="Price Currency" required="" class="select-field-2 w-select">
              <option value="">select currency</option>
              <% currencies.forEach((currency)=>{%>
                <% if(JSON.stringify(currency)==JSON.stringify(appointment['currency'])){ %>
                  <option value="<%= JSON.stringify(currency); %>" selected ><%= currency['fullName']+' ('+currency['symbol'] +')'%></option>
                  <% }else{ %>
                  <option value="<%= JSON.stringify(currency); %>"><%= currency['fullName']+' ('+currency['symbol'] +')'%></option>
                  <% } %>
                <% }); %>
            </select>
            <input type="text" class="text-field-9 appointment-price w-input" maxlength="256" value="<%= appointment['appointmentPrice']%>" name="appointmentPrice" data-name="Appointment Price" placeholder="Price for appointment" id="Appointment-Price" required=""></div>
        </form>
