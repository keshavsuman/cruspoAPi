                let date = new Date(course['courseExpiryDate']);
                  year = date.getFullYear();
                  month = date.getMonth()+1;
                  dt = date.getDate();

                  if (dt < 10) {
                    dt = '0' + dt;
                  }
                  if (month < 10) {
                    month = '0' + month;
                  }
                  let d = year+'-' + month + '-'+dt;%><%=d%><%}%>">
              <img src="/images/calendar-icon.svg" loading="lazy" width="26" alt=""></div>
