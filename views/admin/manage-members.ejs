        <div class="manage-members-data-wrapper">
          <% learners.forEach(l=>{ %>
          <div class="manage-members-data">
            <div class="first-name">
              <div class="text-block-141"><%= l['firstName'] %></div>
            </div>
            <div class="last-name">
