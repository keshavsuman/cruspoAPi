          <div class="add-member-form-block w-form">
            <form id="email-form" name="email-form" data-name="Email Form" class="add-member-form">
              <div class="select-group-wrapper">
                <select id="group" name="group" required="" data-name="group" class="select-existing-group w-select">
                  <option value="">Select one.</option>
                 <% groups.forEach(g=>{ %>  
                    <option value="<%= g['name']%>"><%= g['groupName']%></option>
                  <% }); %>  
                </select></div>
              <div class="member-details-wrapper">
                <input type="text" class="text-field-10 w-input" maxlength="256" name="firstName" data-name="firstName" placeholder="Enter first name" id="firstName" required="">
                <input type="text" class="text-field-11 w-input" maxlength="256" name="lastName" data-name="lastName" placeholder="Enter last name" id="lastName" required=""></div>
              <div class="member-details-wrapper">
                <input type="email" class="text-field-10 w-input" maxlength="256" name="email" data-name="email" placeholder="Enter email address" id="email" required="">
                <input type="tel" class="w-input" maxlength="256" name="contact" data-name="contact" placeholder="Enter contact no" id="contact"></div>
              <div class="member-button">
                <a href="#" class="popup-button w-button">Add Member</a>
              </div>
            </form>
          </div>
        </div>
      </div>
      <div style="display:none" class="create-group-form-box">
        <div class="create-group-form-wrapper">
          <div class="text-block-59">Create New Group</div>
          <div class="new-group-form-block w-form">
            <form id="email-form-2" name="email-form-2" data-name="Email Form 2" class="new-group-form">
              <input type="text" class="group-title w-input" maxlength="256" name="Group-Title" data-name="Group Title" placeholder="Give a title to your group" id="Group-Title" required="">
              <div class="add-member-dropdown-wrapper">
                <div data-hover="" data-delay="0" class="add-member-select-dropdown w-dropdown">
                  <div class="add-member-toggle w-dropdown-toggle">
                    <div class="w-icon-dropdown-toggle"></div>
                    <div class="text-block-60">Add Member</div>
                  </div>
                  <nav class="add-member-dropdown-list w-dropdown-list">
                    <div data-w-id="9942b7a2-3809-48c8-7d40-6ef290929551" class="existing-group">
