<!DOCTYPE html>
<html data-wf-page="5fe34af8753d11393d3b5b00" data-wf-site="5fde0bc9a49dc57bc05fab17">
<head>
  <meta charset="utf-8">
  <title>Resource Library</title>
  <meta content="width=device-width, initial-scale=1" name="viewport">
  <link href="/css/normalize.css" rel="stylesheet" type="text/css">
  <link href="/css/webflow.css" rel="stylesheet" type="text/css">
  <link href="/css/cruspo-live.webflow.css" rel="stylesheet" type="text/css">
  <script type="text/javascript">!function(o,c){var n=c.documentElement,t=" w-mod-";n.className+=t+"js",("ontouchstart"in o||o.DocumentTouch&&c instanceof DocumentTouch)&&(n.className+=t+"touch")}(window,document);</script>
  <link href="/images/favicon.ico" rel="shortcut icon" type="image/x-icon">
  <link href="/images/webclip.png" rel="apple-touch-icon">
</head>
<body>
  <div style="display:none" class="create-folder-form-wrapper">
    <div class="create-folder-form-box">
      <div class="text-block-59">Create New Folder</div>
      <div class="new-group-form-block w-form">
            <form id="email-form-2" name="folderForm" method="POST" action="/admin/resources/createFolder" data-name="Email Form 2" class="new-group-form">
              <input type="text" class="group-title w-input" maxlength="256" name="folderName" data-name="folderName" placeholder="Give a title to your folder" id="folderTitle" required>
            </form>
      </div>
      <div class="create-group-form-button-wrapper">
        <div data-w-id="2d465613-4866-f598-14b9-765901916549" class="group-cancel-button">
          <div>Cancel</div>
        </div>
        <div data-w-id="2d465613-4866-f598-14b9-76590191654c" class="group-create-button">
          <div class="text-block-61">Create </div><img src="/images/dropdown-arrow.svg" loading="lazy" alt="dropdown-arrow" class="image-23">
        </div>
      </div>
    </div>
  </div>
  <div class="course-creator-section">
    <div class="upload-wrapper">
      <div class="upload-files-wrapper">
        
        <!-- <div class="file-uploading-wrapper file">
          <div class="upload-type"><img src="/images/document-type.svg" loading="lazy" alt=""></div>
          <div class="file-name">This is some text inside of a div block.</div>
          <div class="upload-status">
            <div class="upload-percent">This is some text inside of a div block.</div>
            <div class="upload-success"><img src="/images/lesson-complete.svg" loading="lazy" width="20" alt=""></div>
          </div>
          <div class="cancel-upload-button-wrapper">
            <div class="cancel-upload"><img src="/images/cancel-button.svg" loading="lazy" width="20" alt=""></div>
          </div>
        </div> -->
        
      </div>
      <div class="upload-button-wrapper">
        <div style="-webkit-transform:translate3d(0, 0, 0) scale3d(0, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-moz-transform:translate3d(0, 0, 0) scale3d(0, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-ms-transform:translate3d(0, 0, 0) scale3d(0, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);transform:translate3d(0, 0, 0) scale3d(0, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);display:none" class="upload-option-wrapper">
          <!-- <div class="upload-option folder"><img src="/images/folder-icon.svg" loading="lazy" alt="">
            <div class="text-block-117">upload folder</div>
          </div> -->
          <div class="upload-option file"><img src="/images/document-type.svg" loading="lazy" alt="">
            <div class="text-block-117">upload file</div>
          </div>
          <div data-w-id="24820467-b37a-e129-43de-c47b9c8aea74" class="create-folder-button-wrapper"><img src="/images/folder-icon.svg" loading="lazy" alt="">
            <div class="text-block-117">create folder</div><img src="/images/add-member.svg" loading="lazy" width="13" alt="" class="image-29">
          </div>
        </div>
        <div data-w-id="e97d8672-49c3-65cb-2065-1fecfa228a54" class="upload-button">
          <div class="text-block-20">Add New</div><img src="/images/upload-button.svg" loading="lazy" width="26" alt="" class="image-7">
        </div>
      </div>
    </div>
    <div class="resource-library-wrapper">
      <div data-duration-in="300" data-duration-out="100" class="resources-tab w-tabs">
        <div class="resources-tab-menu w-tab-menu">
          <a data-w-tab="Tab 1" class="all-files-link w-inline-block w-tab-link w--current">
            <div>All Files</div>
          </a>
          <a data-w-tab="Tab 3" class="all-folders w-inline-block w-tab-link">
            <div>All Folders</div>
          </a>
        </div>
        <div class="w-tab-content">
          <div data-w-tab="Tab 1" class="all-files-tab w-tab-pane w--tab-active">
            <!-- <div class="storage-indicator-wrapper">
              <div class="storage-indicator"></div>
              <div class="storage-text">This is some text inside of a div block.</div>
            </div> -->
            <div class="resource-library-search-wrapper">
              <form action="/search" class="cruspo-search library w-form"><input type="search" class="search-input-2 w-input" maxlength="256" name="query" placeholder="Search…" id="search" required=""><input type="submit" value="Search" class="search-button-2 w-button"></form>
              <!-- <div class="search-animation">
                <div data-w-id="145df3fa-df92-0347-d87b-9e00f907bdac" data-animation-type="lottie" data-src="/documents/50807-pacman-loading.json" data-loop="0" data-direction="1" data-autoplay="0" data-is-ix2-target="1" data-renderer="svg" data-default-duration="2.8" data-duration="0" class="animation"></div>
              </div> -->
            </div>
            <div class="all-files-wrapper">
              <div class="files-box">
                <div class="recent-files-grid">
                  <% for(var i=0;i<4;i++){ %>
                  <div class="recent-files-wrapper">
                    <img class="recent-files-thumbnail" src="<%= files[i]['fileURL']%>">
                    <div class="div-block-10">
                      <div><%= files[i]['fileName']%></div>
                    </div>
                  </div>
                  <%};%>
                </div>
              </div>
            </div>
            <div class="all-file-list-wrapper">
              <%files.forEach(f=>{%>
              <div class="files">
                <a href="#" class="file-wrapper w-inline-block"><img src="/images/document-type.svg" loading="lazy" alt="">
                  <div class="file-title"><%= f['fileName']%></div>
                </a>
                <div class="options">
                  <div class="file-other-options">
                    <div class="download-button folder"><img src="/images/download-icon.svg" loading="lazy" width="21" alt=""></div>
                    <div class="file-info-button"><img src="/images/info-icon.svg" loading="lazy" width="15" alt=""></div>
                  </div>
                  <div class="file-options-wrapper">
                    <div data-w-id="34a1de63-8a33-d450-f76c-32fb0c51be27" class="file-option-menu">
                      <div class="file-option-button"></div>
                      <div class="file-option-button"></div>
                      <div class="file-option-button"></div>
                    </div>
                    <div class="file-options-list">
                      <div data-w-id="34a1de63-8a33-d450-f76c-32fb0c51be2c" class="file-option share">
                        <div class="text-block-118">share</div><img src="/images/share-button.svg" loading="lazy" width="12" alt="" class="image-31">
                      </div>
                      <div class="file-option">
                        <div class="text-block-118">copy</div><img src="/images/copy-icon.svg" loading="lazy" width="10" alt="">
                      </div>
                      <div class="file-option">
                        <div class="text-block-118">move</div><img src="/images/send-feedback.svg" loading="lazy" width="11" alt="" class="image-31">
                      </div>
                      <div class="file-option">
                        <div class="text-block-118">delete</div><img src="/images/delete.svg" loading="lazy" width="10" alt="" class="image-31">
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <%});%>
            </div>
          </div>
          <div data-w-tab="Tab 3" class="all-folders-tab w-tab-pane">
            <div class="resource-library-search-wrapper">
              <form action="/search" class="cruspo-search library w-form"><input type="search" class="search-input-2 w-input" maxlength="256" name="query" placeholder="Search…" id="search" required=""><input type="submit" value="Search" class="search-button-2 w-button"></form>
            </div>
            <div class="all-folders-list-wrapper">
              <%folders.forEach(f=>{%>
              <div class="folder">
                <a href="/admin/resources/<%= f['folderName'] %>" class="folder-wrapper w-inline-block"><img src="/images/folder-icon.svg" loading="lazy" alt="">
                  <div class="folder-title"><%= f['folderName'] %></div>
                </a>
                <div class="options">
                  <div class="file-other-options">
                    <div class="download-button folder"><img src="/images/download-icon.svg" loading="lazy" width="21" alt=""></div>
                    <div class="file-info-button"><img src="/images/info-icon.svg" loading="lazy" width="15" alt=""></div>
                  </div>
                  <div class="file-options-wrapper">
                    <div data-w-id="0060b66c-9c25-29c5-ff01-f4dc49db032d" class="file-option-menu">
                      <div class="file-option-button"></div>
                      <div class="file-option-button"></div>
                      <div class="file-option-button"></div>
                    </div>
                    <div class="file-options-list">
                      <div class="file-option">
                        <div class="text-block-118">share</div><img src="/images/share-button.svg" loading="lazy" width="12" alt="" class="image-31">
                      </div>
                      <div class="file-option">
                        <div class="text-block-118">copy</div><img src="/images/copy-icon.svg" loading="lazy" width="10" alt="">
                      </div>
                      <div class="file-option">
                        <div class="text-block-118">move</div><img src="/images/send-feedback.svg" loading="lazy" width="11" alt="" class="image-31">
                      </div>
                      <div class="file-option">
                        <div class="text-block-118">delete</div><img src="/images/delete.svg" loading="lazy" width="10" alt="" class="image-31">
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <%});%>
            </div>
          </div>
        </div>
      </div>
    </div>
    <input type="file" name="file" class="fileUpload" style="display: none;" id="">
    <div class="course-creator-other-menu-wrapper">
      <div style="-webkit-transform:translate3d(0, 0, 0) scale3d(0, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-moz-transform:translate3d(0, 0, 0) scale3d(0, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-ms-transform:translate3d(0, 0, 0) scale3d(0, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);transform:translate3d(0, 0, 0) scale3d(0, 0, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);display:none" class="share-info-wrapper">
        <div class="share-info-header">
          <div class="text-block-121">link copied</div><img src="/images/success-blacck.svg" loading="lazy" width="14" alt="">
        </div>
        <div class="text-block-120 course">link copied to clipboard</div>
      </div>
      <div class="course-creator-notification">
        <div data-w-id="8aa830b5-0f02-9017-c97a-a84459b36894" class="course-creator-notification-icon-wrapper">
          <div class="course-creator-notifiaction-icon"></div>
        </div>
        <div style="-webkit-transform:translate3d(0, 0, 0) scale3d(0.01, 0.01, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-moz-transform:translate3d(0, 0, 0) scale3d(0.01, 0.01, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-ms-transform:translate3d(0, 0, 0) scale3d(0.01, 0.01, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);transform:translate3d(0, 0, 0) scale3d(0.01, 0.01, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);display:none" class="course-creator-notification-content-wrapper">
          <% if(notifications.length==0){%>
            <a href="#" class="course-creator-notification-content w-inline-block">
              <div>No Notifications</div>
            </a>  
          <%}%>
          <% notifications.forEach(notification=>{%>
          <a href="#" class="course-creator-notification-content w-inline-block">
            <img src="/images/cruspo-logo-non-text.svg" loading="lazy" width="42" alt="" class="image-5">
            <div><% notification['notificationTitle']%></div>
          </a>
         <% }) %>
        </div>
      </div>
    </div>
    <%- include('sidebar');%>
  </div>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.21.1/axios.min.js" integrity="sha512-bZS47S7sPOxkjU/4Bt0zrhEtWx0y0CRkhEp8IckzK+ltifIIE9EMIMTuT/mEzoIMewUINruDBIR/jJnbguonqQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=5fde0bc9a49dc57bc05fab17" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
  <script src="/js/webflow.js" type="text/javascript"></script>
  <script src="/js/resourceLibrary.js" type="text/javascript"></script>
  <script>
    $('.upload-files-wrapper').hide();
  </script>
</body>
</html>