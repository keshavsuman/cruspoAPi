  <link href="/images/webclip.png" rel="apple-touch-icon">
</head>
<body class="body-2">
  <div class="unenroll-form-wrapper">
    <div class="unenroll-form-box">
      <div class="text-block-115">Are you sure you want to unenroll from the course?</div>
      <div class="text-block-116">*you will not be able to access the course content and you will not receive any updates about the course. </div>
      <div class="unenroll-live-course-cancel-button-wrapper">
        <div data-w-id="297dc621-bb24-348d-a731-dcaeb7a98754" class="unenroll-course-cancel-button">
          <div>Cancel</div><img src="/images/add-member.svg" loading="lazy" width="14" alt="" class="cancel-icon">
        </div>
      </div>
      <div class="unenroll-button-wrapper">
        <div data-w-id="993ba64b-5eae-0402-03b7-67eed709aa33" class="unenroll-course">
          <div>No, I want to stay here</div>
        </div>
        <div class="unenroll-course unenroll">
          <div>Yes, I want to leave</div>
        </div>
      </div>
    </div>
  </div>
  <div class="learner-section wf-section">
    <div class="learner-dashboard-wrapper">
      <h1 class="heading-19">My Programs</h1>
      <% if(purchasedCourses.length==0){%>
      <div class="no-courses-wrapper learner">
        <div class="text-block-31">You haven&#x27;t purchased any program so far. Enroll into your first program by clicking the button below</div>
        <div class="view-all-courses-button-wrapper">
          <a href="/learner-only-areas/learner-courses.html" class="cruspo-temp-button view-all-courses w-inline-block">
            <div class="text-block-4">View All Programs</div><img src="/images/arrow-cruspo.svg" loading="lazy" alt="">
          </a>
        </div>
      </div>
      <%}%>
      <div class="w-layout-grid learner-dasboard-course-grid">
        <% purchasedCourses.forEach(c=>{ %>
        <div class="learner-dashboard-courses-overview-wrapper">
          <div class="dashboard-courses-overview">
            <div class="dashboard-courses-thumbnail">
              <img src="<%=c['courseThumbnail']%>" loading="lazy" width="122" alt="" class="thumbnail-image"></div>
            <div class="dashboard-courses-content-wrapper">
              <h2 class="course-title"><%=c['courseTitle']%></h2>
              <div class="course-description-text"><%=c['feeDetails']['price']%> <%=c['feeDetails']['currency']['symbol']%></div>
              <div class="dashboard-courses-cta-wrapper">
                <a href="/learn/program/<%=c['_id']%>" class="cruspo-temp-button edit-course w-inline-block">
                  <div class="text-block-4">View Course</div><img src="/images/arrow-cruspo.svg" loading="lazy" alt="">
                </a>
                <!-- <a data-w-id="37b6f0bf-7d96-31cc-24ca-1301887475e7" href="#" class="cruspo-temp-button unenroll w-inline-block">
                  <div class="text-block-4">Unenroll</div><img src="/images/log-out.svg" loading="lazy" alt="" class="image-28">
                </a> -->
              </div>
            </div>
          </div>
        </div>
        <% });%>

      </div>
      <div class="view-all-courses-button-wrapper">
        <a href="/learn/course" class="cruspo-temp-button view-all-courses w-inline-block">
          <div class="text-block-4">View All Courses</div><img src="/images/arrow-cruspo.svg" loading="lazy" alt="">
        </a>
      </div>
      <div class="dashboard-go-live-wrapper learner">
        <h3 class="heading-2">Upcoming Live Sessions</h3>
        <div class="upcoming-live-sessions-details">
          <div class="div-block-7">
            <h4 class="heading-3">Live Session Name</h4>
            <div class="share-course-button-wrapper">
              <a href="#" class="share-live-session-button w-inline-block">
                <div class="text-block-4 share">Share Info</div><img src="/images/share-button.svg" loading="lazy" alt="" class="image-4">
              </a>
            </div>
          </div>
        </div>
        <div class="live-session-schedule-data">
          <div class="live-session-schedule-day-and-date">
            <div class="text-block-8">Monday</div>
            <div class="text-block-8">13 august 2021</div>
          </div>
          <div class="live-session-schedule-time">
            <div>9:00 AM (IST)</div>
          </div>
        </div>
        <div class="live-session-group-data-wrapper"></div>
        <!-- <div class="upcoming-live-button-wrapper">
          <a href="#" class="cruspo-temp-button edit-live-session w-inline-block">
            <div class="text-block-4">Edit Session</div><img src="/images/pen.svg" loading="lazy" width="14" alt="">
          </a>
          <a href="#" class="cruspo-temp-button view-all-live-sessions w-inline-block">
            <div class="text-block-4">View all Sessions</div><img src="/images/eye-icon.svg" loading="lazy" width="20" alt="">
          </a>
        </div> -->
      </div>
      <div class="learner-dashboard-billing-history-box">
        <h3 class="heading-6">Billing Overview</h3>
        <ul role="list" class="recent-billing-learner-dashboard-list w-list-unstyled">
          <li class="recent-billing-item-learner"></li>
          <li class="recent-billing-item-learner"></li>
          <li class="recent-billing-item-learner"></li>
        </ul>
        <div class="billing-overview-button-wrapper">
          <a href="#" class="cruspo-temp-button view-all-earnings w-inline-block">
            <div class="text-block-4">View Complete Billing</div><img src="/images/arrow-cruspo.svg" loading="lazy" alt="">
          </a>
        </div>
      </div>
    </div>
  </div>
  <div class="course-creator-menu-wrapper learner">
    <div class="learner-menu-hide-button-wrapper">
      <div class="learner-menu-button">
        <div data-w-id="21735758-1f9e-ca58-80de-97620cce79bc" class="learner-menu-hide-arrow"><img src="/images/dropdown-arrow.svg" loading="lazy" width="19" alt="dropdown-arrow
" class="image-15"></div>
      </div>
    </div>
    <%- include('sidebar')%>
  </div>
  <%//include('sidebar')%>
  <div class="learner-bottom-menu">
    <div class="learner-more-option-wrapper">
      <div class="learner-more-window">
        <a href="/learn/account" class="learner-menu-link account more w-inline-block"><img src="/images/account-on-click.svg" loading="lazy" width="20" alt="" class="image-16 more">
          <div class="text-block-41 bottom">My Account</div>
        </a>
        <a href="/learn/help-and-support" class="learner-menu-link help-and-support more w-inline-block"><img src="/images/help-and-support.svg" loading="lazy" width="20" alt="" class="image-16 more">
          <div class="text-block-41 bottom">Help &amp; Support</div>
        </a>
        <a href="/authentication/logout" class="learner-menu-link log-out w-inline-block"><img src="/images/log-out.svg" loading="lazy" width="20" alt="" class="image-16 more-copy">
          <div class="text-block-41 bottom">Log Out</div>
        </a>
      </div>
    </div>
    <div class="learner-menu-link-wrapper bottom-menu">
      <a href="/learn/" aria-current="page" class="learner-menu-link dashboard bottom w-inline-block w--current"><img src="/images/dashboard.svg" loading="lazy" width="22" alt="" class="image-16">
        <div class="text-block-41 bottom">Dashboard</div>
      </a>
      <a href="/learn/programs" class="learner-menu-link courses bottom w-inline-block"><img src="/images/course-on-click.svg" loading="lazy" width="17" alt="" class="image-16">
        <div class="text-block-41 bottom">My Programs</div>
      </a>
      <a href="/learn/live" class="learner-menu-link live bottom w-inline-block"><img src="/images/cruspo-live-stream.svg" loading="lazy" width="21" alt="" class="image-16">
        <div class="text-block-41 bottom">Live Sessions</div>
      </a>
      <!-- <a href="/learner-only-areas/learner-billing.html" class="learner-menu-link billing bottom w-inline-block"><img src="/images/billing-history.svg" loading="lazy" width="19" alt="" class="image-16">
        <div class="text-block-41 bottom">Billing</div>
      </a> -->
      <div data-w-id="b1e02a9a-019a-502a-afbe-ee0bc31d2492" class="learner-menu-more">
        <div class="learner-menu-more-button">
          <div class="more-button learner"></div>
          <div class="more-button learner"></div>
          <div class="more-button learner"></div>
        </div>
      </div>
    </div>
  </div>
  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=5fde0bc9a49dc57bc05fab17" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
  <script src="/js/webflow.js" type="text/javascript"></script>
  <script>
    $('.share-live-session-button').click(function(){
      alertify.notify("Copied to clipboard","success");
    });
  </script>
</body>
</html>