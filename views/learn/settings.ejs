    <div class="learner-setting-wrapper">
      <h1 class="heading-25">Account Settings</h1>
      <div class="account-settings-form-block learner w-form">
        <form id="email-form" name="email-form" data-name="Email Form" class="account-settings-form">
          <div class="name-wrapper">
            <input type="text" class="text-field-3 w-input" maxlength="256" name="firstName" value="<%=user['firstName']%>" data-name="Creator First Name" placeholder="First Name" id="creatorFirstName">
            <input type="text" class="text-field-2 w-input" maxlength="256" name="lastName" value="<%=user['lastName']%>" data-name="Creator Last Name" placeholder="Last Name" id="creatorLastName"></div>
          <div class="username-wrapper"></div>
          <div class="account-email">
            <input type="email" class="text-field-4 w-input" maxlength="256" name="email" value="<%=user['email']%>" data-name="Creator Email" placeholder="Enter Your Best Email Address" id="creatorEmail" required="">
            <% if(user['isVerified']){ %>
            <div class="cruspo-button-2 account verified">
              <div>Verified</div>
            </div>
            <% }else{ %>
              <div class="cruspo-button-2 account verified">
                <div>Not Verified</div>
              </div>
            <% } %>
            </div>
